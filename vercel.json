{
  "version": 2,
  "functions": {
    "api/sendEmail.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/sendEmail",
      "dest": "api/sendEmail.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
